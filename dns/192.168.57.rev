; 57.168.192.in-addr.arpa
;
; BIND reverse data file for local loopback interface
$TTL	86400
@	IN	SOA	57.168.192.in-addr.arpa. root.sri.ies. (
			      1		; Serial
			 604800		; Refresh
			  86400		; Retry
			2419200		; Expire
			   7200 )	; Negative Cache TTL
;
; name servers (NS)
@		IN		NS		ns.sri.ies.
;
; hosts (A)
10		IN		PTR		ns.sri.ies.
20		IN      PTR     mirror.sri.ies.
30		IN      PTR     ftp.sri.ies.
;